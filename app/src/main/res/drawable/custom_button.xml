<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Button background color -->
    <solid android:color="#FFFFFF"/>

    <!-- Blunt corners -->
    <corners android:radius="20dp"/>

    <!-- Optional: padding inside the button -->
    <padding
        android:left="10dp"
        android:top="5dp"
        android:right="10dp"
        android:bottom="5dp"/>
</shape>
